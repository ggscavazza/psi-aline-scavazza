{
  "extends": "@react-native/typescript-config/tsconfig.json",
  "compilerOptions": {
    "jsx": "react-native",
    "module": "node16",
    "moduleResolution": "node16",
    "allowJs": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "strict": false,
    "target": "es6",
    "resolveJsonModule": true,
    "lib": ["es6", "dom", "dom.iterable", "es2017", "esnext"],
    "allowSyntheticDefaultImports": true,
    "types": ["webpack-env"]
  },
  "include": [
    "index.web.js",
    "index.ts",
    "index.tsx",
    "App.tsx",
    "App.web.tsx ",
    "src"
  ],
  "exclude": [
    "node_modules",
    "babel.config.js",
    "metro.config.js",
    "webpack.config.js"
  ]
}
